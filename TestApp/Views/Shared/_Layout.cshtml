<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - TestApp</title>
    <link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="~/css/_responsiveTable.min.css" />
</head>
<body>
    <header>
        <nav class="w3-bar w3-mobile w3-white">
            <div class="w3-container">
                <a class="w3-bar-item" asp-area="" asp-page="/Index">Home</a>
                <a class="w3-bar-item" asp-area="" asp-page="/Privacy">Privacy</a>
            </div>
        </nav>
    </header>
    <div class="w3-container">
        <main role="main" class="w3-padding-small">
            @RenderBody()
        </main>
    </div>

    <footer class="w3-border-top w3-opacity">
        <div class="w3-container">
            &copy; 2022 - TestApp - <a asp-area="" asp-page="/Privacy">Privacy</a>
        </div>
    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>
